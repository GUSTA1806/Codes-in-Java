public class Carro extends Veiculo{

    public Carro(String marca, String modelo){
        super(marca, modelo);
    }

    public void buzinar(){
         System.out.println("Buzina: Biiiiii");
    }

    public void mostrarInf(){
        System.out.println("Carro:" +marca+ "-" +modelo+ "");
    }
}
