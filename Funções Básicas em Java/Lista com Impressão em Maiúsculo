import java.util.ArrayList;
import java.util.List;

public class Main {
    public static void main(String[] args) {
        List<String> names = new ArrayList<>();
        names.add("Ana");
        names.add("Carlos");
        names.add("Marcos");
        names.add("Julia");

        for (String name : names) {
            System.out.println(name.toUpperCase());
        }
    }
}
